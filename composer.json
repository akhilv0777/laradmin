{
  "name": "akhilv/laradmin",
  "description": "Custom admin dashboard + auth + role system",
  "type": "library",
  "require": {
    "php": "^8.2",
    "illuminate/support": "^11.0|^12.0",
    "spatie/laravel-permission": "^6.7"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "autoload": {
    "psr-4": {
      "Akhilesh\\Laradmin\\": "src/"
    },
    "files": [
      "src/Support/helpers.php"
    ]
  },
  "extra": {
    "laravel": {
      "providers": [
        "Akhilesh\\Laradmin\\LaradminServiceProvider"
      ]
    }
  }
}